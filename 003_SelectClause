use sql_store;

select 
	first_name, 
    last_name, 
    points, 
    points + 10 as 'sql expression'
from customers;

select distinct state
from customers;

-- Return all the products
-- name, unit price, new price (unit price * 1.1)

select 
	name, 
    unit_price, 
    unit_price * 1.1 as 'new price'
from products;
